{
  "name": "electron-app",
  "version": "1.0.0",
  "description": "Minimal React App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "server": "nodemon ./server/app.js",
    "dev": "webpack-dev-server --config ./webpack/webpack.dev.config.js --mode development",
    "build:assets:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.dev.config.js --mode development --watch",
    "build:assets": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.prod.config.js --mode production",
    "lint": "eslint ./src",
    "flow": "flow",
    "test": "npm run flow && npm run lint",
    "package:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package:win": "electron-packager . paysight --overwrite --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"PaySight\"",
    "package:win:installer": "node installers/windows/createinstaller.js",
    "package:linux": "electron-packager . electron-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": "Wilson Anciro",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "apollo-boost": "^0.3.1",
    "autoprefixer": "^9.5.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^2.0.2",
    "compression-webpack-plugin": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "^5.0.1",
    "electron-packager": "^13.1.1",
    "electron-winstaller": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-flowtype": "^3.7.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.98.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.4",
    "react-monaco-editor": "^0.25.1",
    "react-router-dom": "^5.0.0",
    "sass-loader": "^7.1.0",
    "style-resources-loader": "^1.2.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.8",
    "express": "^4.16.4",
    "express-graphql": "^0.8.0",
    "express-session": "^1.16.1",
    "fs-extra": "^8.0.1",
    "graphql": "^0.13.0",
    "moment": "^2.24.0",
    "multer": "^1.4.1",
    "node-adodb": "^5.0.2"
  }
}
